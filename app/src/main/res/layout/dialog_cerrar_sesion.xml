<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/dialog_primary"
    android:background="@color/backgroundColor"
    android:padding="10dp">

    <TextView
        android:id="@+id/tvConfirmarCierreSesion"
        style="@style/tv_primary_bold_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:fontFamily="sans-serif-smallcaps"
        android:text="@string/cerrar_sesion"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/tvCuerpoConfirmarCierreSesion"
        style="@style/tv_primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tvConfirmarCierreSesion"
        android:layout_centerHorizontal="true"
        android:fontFamily="sans-serif-smallcaps"
        android:text="@string/cerrar_sesion_cuerpo"
        android:textAlignment="center" />


</RelativeLayout>